{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "google"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "strict": 0,
        "require-jsdoc": 0,
        "linebreak-style": ["error", "windows"],
        "max-len": ["error", { "code": 10000 }],
        "eol-last": ["error", "never"],
        "comma-dangle": ["error", "never"],
        "no-multi-spaces": ["error", { "exceptions": { "ImportDeclaration": true, "VariableDeclarator": true } }],
        "new-cap": ["error", { "capIsNewExceptions": ["Router"], "newIsCapExceptions": ["utc"] }]
    }
}
